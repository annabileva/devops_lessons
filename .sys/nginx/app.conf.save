http {
	server {
		listen 80 default_server;
		listen [::]:80 default_server;

		root /home/BilevaAA/devops_lessons/static-files;

	index index.html;

	server_name _;

	location / {
		try_files $uri $uri/ =404;
	}

	location /node {
		proxy_pass http://127.0.0.1:3000;
	}

	location /php {
		include fastcgi.conf;
		fastcgi_param   SCRIPT_FILENAME /home/BilevaAA/devops_lessons/php-app/index.php;
		fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
	}
        location /

}


